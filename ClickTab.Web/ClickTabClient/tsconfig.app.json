/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "strictPropertyInitialization": false,
    "types": [],
    "paths": {
      "@docs-components/*": [
        "./src/components/*"
      ],
      "@angular/*": [
        "../node_modules/@angular/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "**/*.d.ts"
  ]
}
